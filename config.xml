<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="com.designge.dronercgeo" versionCode="1" version="0.0.1" package="com.designge.dronercgeo">
  <name>Drone RC</name>
  <icon src="www/icon_1024.png"/>
  <icon src="www/icon_1024.png" width="1024" height="1024"/>
  <allow-intent href="*"/>
  <allow-navigation href="*"/>
  <allow-navigation href="*://*.mixshop.ge/*"/>
  <allow-intent href="http://*/*" launch-external="yes"/>
  <allow-intent href="https://*/*" launch-external="yes"/>
  <platform name="android">
    <preference name="android-minSdkVersion" value="20"/>
    <allow-intent href="market:*"/>
  </platform>
  <preference name="android-build-tool" value="gradle"/>
  <!--<preference name="StatusBarOverlaysWebView" value="true" />-->
  <preference name="phonegap-version" value="cli-9.0.0"/>
  <preference name="target-device" value="handset"/>
  <preference name="webviewbounce" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="false"/>
  <preference name="auto-hide-splash-screen" value="false"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="20"/>
  <preference name="android-installLocation" value="auto"/>
  <preference name="EnableViewportScale" value="true"/>
  <preference name="StatusBarOverlaysWebView" value="true"/>
  <preference name="StatusBarBackgroundColor" value="#00000"/>
  <preference name="StatusBarStyle" value="lightcontent"/>
  <description>
Drone Remote Control
    </description>
  <author href="http://design.ge" email="uabashidze@gmail.com">
        Cyborg-Mcloud
    </author>
  <plugins>
    <feature name="App">
      <param name="android-package" value="com.designge.dronercgeo"/>
    </feature>
    <!--	<feature name="http://api.phonegap.com/1.0/device" />-->
    <!--	<gap:plugin name="cordova-plugin-inappbrowser"  source="npm" />-->
    <gap:plugin name="cordova-plugin-whitelist" source="npm"/>
    <!--	<plugin name="cordova.plugins.diagnostic" source="npm" />-->
    <!--	<plugin name="cordova-plugin-dialogs" spec="^1.3.4"/>-->
    <!--	<plugin name="cordova-plugin-device" source="npm" />-->
    <gap:plugin name="cordova-plugin-uniquedeviceid" source="npm"/>
    <plugin name="cordova-android-support-gradle-release" spec="^1.1.1">
      <variable name="ANDROID_SUPPORT_VERSION" value="26.0.1"/>
    </plugin>
  </plugins>
  <preference name="WindowsStorePublisherName" value="Cyborg-Mcloud"/>
  <preference name="WindowsStoreIdentityName" value="1c217da8-6dfc-68e2-36eb-5847b610ec8b"/>
  <access origin="*"/>
  <preference name="KeepRunning" value="false"/>
  <preference name="monaca:AndroidIsPackageNameSeparate" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="Orientation" value="landscape"/>
  <preference name="SplashScreenDelay" value="0"/>
  <preference name="Fullscreen" value="true"/>
</widget>
